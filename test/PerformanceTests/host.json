{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "maxConcurrentOrchestratorFunctions": 8,
      "maxConcurrentActivityFunctions": 8,
      "storageProvider": {
        "connectionStringName": "SQLDB_Connection"
      }
    }
  },
  "logging": {
    "logLevel": {
      "default": "Warning",
      "DurableTask.SqlServer": "Warning",
      "DurableTask.Core": "Warning"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false
      }
    }
  }
}